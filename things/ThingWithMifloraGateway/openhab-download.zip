		jQuery(".helpbubble.mavencentral").hover(function () {
			clearTimeout(mcBubbleTimeout);
		}, function () {
			mcBubbleTimeout = setTimeout(function () {
				closeMcBubble();
			}, 1000);
		})

		function closeMcBubble() {
			mcBubble.css("display", "none");
		}


    </script>
